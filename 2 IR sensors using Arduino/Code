// Define IR sensor pins
const int irSensor1 = 7;  // First IR sensor OUT pin
const int irSensor2 = 8;  // Second IR sensor OUT pin

void setup() {
  Serial.begin(9600);
  pinMode(irSensor1, INPUT);
  pinMode(irSensor2, INPUT);
}

void loop() {
  int sensor1State = digitalRead(irSensor1);
  int sensor2State = digitalRead(irSensor2);

  Serial.print("IR Sensor 1: ");
  if (sensor1State == LOW) {
    Serial.print("Object Detected");
  } else {
    Serial.print("No Object");
  }

  Serial.print(" | IR Sensor 2: ");
  if (sensor2State == LOW) {
    Serial.println("Object Detected");
  } else {
    Serial.println("No Object");
  }

  delay(300);  // Short delay for readability
}
